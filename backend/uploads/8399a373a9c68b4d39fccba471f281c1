//count the number of elements in given array 
//greater than the given number of x.

#include<iostream>
using namespace std;
int main(){
    int n;
    cout<<"Enter a Number: ";
    cin>>n;
    int arr[n];
    cout<<"Enter the elements of the array: ";
    for(int i=0;i<=n-1;i++){
        cin>>arr[i];
    }
    int x,count = 0;
    cout<<"Enter the value of x: ";
    cin>>x;
    for(int i=0;i<=n-1;i++){
        if(arr[i]>x){
            count++;
        }
    }
    cout<<count<<endl;
}